{
    "name": "nyoncode/laravel-modulio",
    "description": "Laravel modular package system",
    "authors": [
        {
            "name": "Ondřej Nyklíček",
            "email": "ondrej@nyoncode.cz",
            "role": "Developer"
        }
    ],
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=8.2",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/database": "^10.0|^11.0|^12.0",
        "illuminate/cache": "^10.0|^11.0|^12.0",
        "spatie/laravel-permission": "^5.0|^6.0",
        "livewire/livewire": "^3.0",
        "nyoncode/laravel-package-toolkit": "^1.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "orchestra/testbench": "^10.6",
        "pestphp/pest": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "NyonCode\\LaravelModulio\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "NyonCode\\LaravelModulio\\Providers\\ModulioServiceProvider"
            ],
            "aliases": {
                "Modulio": "NyonCode\\LaravelModulio\\Facades\\Modulio"
            }
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
